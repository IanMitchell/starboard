{
	"name": "starboard",
	"version": "1.0.0",
	"description": "A Discord Starboard Bot",
	"author": "Ian Mitchell",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"clean": "rimraf dist",
		"build:watch": "tsc --watch",
		"build": "npm run clean && tsc",
		"start": "cross-env DEBUG_COLORS=true DEBUG=*,-avvio,-follow-redirects,-agentkeepalive node --experimental-specifier-resolution=node dist",
		"debug": "cross-env DEBUG_COLORS=true DEBUG=*,-avvio node --nolazy --inspect --experimental-specifier-resolution=node dist",
		"typecheck": "tsc --noEmit",
		"lint": "eslint src --ext .ts",
		"test": "jest",
		"prettier": "prettier . --check"
	},
	"dependencies": {
		"@discordjs/builders": "^1.1.0",
		"@fastify/cors": "^8.1.0",
		"@logdna/logger": "^2.6.6",
		"@prisma/client": "^4.2.0",
		"@sentry/node": "^7.10.0",
		"chalk": "^5.0.1",
		"cross-env": "^7.0.3",
		"debug": "4.3.4",
		"discord.js": "^14.2.0",
		"discord-api-types": "^0.37.2",
		"dotenv": "^16.0.1",
		"fastify": "^4.4.0",
		"node-cache": "^5.1.2",
		"prom-client": "^14.0.1",
		"date-fns": "2.29.1"
	}
}
