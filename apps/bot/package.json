{
	"name": "starboard",
	"version": "1.0.0",
	"description": "A Discord Starboard Bot",
	"author": "",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"clean": "rimraf dist",
		"build:watch": "tsc --watch",
		"build": "tsc",
		"start": "cross-env DEBUG_COLORS=true DEBUG=*,-avvio node --experimental-specifier-resolution=node dist",
		"debug": "cross-env DEBUG_COLORS=true DEBUG=*,-avvio node --nolazy --inspect --experimental-specifier-resolution=node dist",
		"typecheck": "tsc --noEmit",
		"lint": "eslint . --ext .ts",
		"test": "jest",
		"prettier": "prettier . --check"
	},
	"dependencies": {
		"@discordjs/builders": "^0.14.0",
		"@fastify/cors": "^7.0.0",
		"@logdna/logger": "^2.6.4",
		"@prisma/client": "^3.14.0",
		"@sentry/node": "^6.19.7",
		"chalk": "^5.0.1",
		"cross-env": "^7.0.3",
		"debug": "4.3.4",
		"discord.js": "^13.7.0",
		"discord-api-types": "^0.33.5",
		"dotenv": "^16.0.1",
		"fastify": "^3.29.0",
		"ioredis": "^5.0.4",
		"node-cache": "^5.1.2",
		"prom-client": "^14.0.1"
	}
}
